#!/bin/sh
#SBATCH -N 1
#SBATCH -n 1
#SBATCH -c 116
#SBATCH --output=results/%j.out
#SBATCH --error=results/%j.err
#SBATCH --job-name=chvatal_smt
#SBATCH --time=12:00:00

formulations=("inf" "opt" "red" "inf_sat")

for formulation in "${formulations[@]}"; do 
    for n in {5..7}; do
        srun -c 1 python -m chvatal_smt --quiet "$formulation" "$n" &
    done
done

wait
